{
  "name": "jethrodaniel.github.io",
  "description": "personal website",
  "homepage": "https://jethrodaniel.github.io",
  "repository": "git@github.com:jethrodaniel/jethrodaniel.github.io.git",
  "author": "Mark Delk <jethrodaniel@gmail.com>",
  "license": "UNLICENSED",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "coffee": "coffee",
    "cake": "cake",
    "gh-pages": "gh-pages",
    "test": "node scripts/test.js --env=jsdom --watchAll=false"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "coffeescript": "^2.4.1",
    "moment": "^2.24.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-typing-animation": "^1.6.2",
    "reactstrap": "^8.4.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.3.0",
    "babel-runtime": "^6.26.0",
    "coffee-loader": "^0.9.0",
    "css-loader": "^3.4.2",
    "gh-pages": "^2.1.1",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx,coffee}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx,coffee}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|ts|coffee)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|ts|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(jsts)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "json",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
